  "scripts": {
    "start": "cross-env NODE_ENV=development node dist/index.js",
    "dev": "concurrently -k \"tsc -w\" \"sleep 2 && NODE_ENV=development nodemon ./dist/index.js\"",
    "build": "tsc",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "watch": "NODE_ENV=development tsc -w",
    "type-check": "tsc --pretty --noEmit",
    "typeProd": "NODE_ENV=production tsc",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint \"./src/**/*.ts\" --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts"
  },
