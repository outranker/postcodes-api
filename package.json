    "concurrently": "^7.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "typescript": "^4.2.4"
  }
